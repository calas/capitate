--- 
cwd: <%= current_path %>
log_file: log/mongrel.log
port: "<%= mongrel_port %>"
environment: production
address: 127.0.0.1
pid_file: <%= mongrel_pid_dir %>/mongrel.pid
servers: <%= mongrel_size %>
user: <%= user %>
group: <%= user %>
config_script: <%= mongrel_config_script unless mongrel_config_script.blank? %>
